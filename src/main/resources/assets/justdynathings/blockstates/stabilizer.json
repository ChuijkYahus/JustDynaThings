{
  "multipart": [
    {
      "when": {
        "active": "true",
        "facing": "down"
      },
      "apply": {
        "model": "justdynathings:block/stabilizer/x/on"
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "up"
      },
      "apply": {
        "model": "justdynathings:block/stabilizer/x/on",
        "x": 180
      }
    },
    {
      "when": {
        "active": "true",
        "facing": "north"
      },
      "apply": {
        "model": "justdynathings:block/stabilizer/x/on",
        "x": 270
